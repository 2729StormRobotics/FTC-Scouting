<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.stormscouter"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.stormscouter.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.stormscouter.AutoActivity"
            android:label="@string/title_activity_auto" >
        </activity>
        <activity
            android:name="com.example.stormscouter.TeleEndActivity"
            android:label="@string/title_activity_tele_end" >
        </activity>
        <activity
            android:name="com.example.stormscouter.QRActivity"
            android:label="@string/title_activity_qr" >
        </activity>
    </application>

</manifest>
